{
    "name": "helhum/t3satis",
    "description": "Repository Generator with support for TYPO3 CMS extension (git) repositories",
    "keywords": ["package", "repository", "generator", "TYPO3 CMS"],
    "homepage": "https://github.com/helhum/t3satis",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Helmut Hummel",
            "email": "info@helmut-hummel.de",
            "homepage": "http://typo3.helmut-hummel.de"
        }
    ],
    "bin": ["bin/t3satis"],
    "require": {
        "php": ">=5.3.2",
        "composer/satis": "dev-master@dev",
        "composer/composer": "1.0.*@dev",
        "guzzlehttp/guzzle": "^6.0"
    },
    "autoload": {
        "psr-0": { "Helhum\\T3Satis": "Classes/" }
    },
    "autoload-dev": {
        "psr-4": { "Helhum\\T3Satis\\Test\\": "Tests/" }
    }
}
